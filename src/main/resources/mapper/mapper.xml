<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaoqian.demo.mapper.UserMapper">
    <insert id="add">
        INSERT INTO User(username,password,age,sex,email,address)
        VALUES (#{username},#{password},#{age},#{sex},#{email},#{address})
    </insert>

    <update id="update">

    </update>

    <select id="findAll" resultType="com.xiaoqian.demo.entity.User">
        SELECT * FROM User
    </select>

    <select id="selectById" resultType="com.xiaoqian.demo.entity.User">
        SELECT * FROM user
        WHERE id=#{id}
    </select>

</mapper>